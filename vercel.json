{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "dist/index.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    {
      "source": "(.*)",
      "destination": "https://mern-project-with-react-flow.vercel.app/$1"
    },
    { "source": "(.*)", "destination": "/index.html" }
  ]
}
