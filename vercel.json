{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "dist/index.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "(.*)", "destination": "dist/index.js" },
    { "source": "(.*)", "destination": "/index.html" },
    {
      "source": "/proxy/:match*",
      "destination": "https://mern-project-with-react-flow.vercel.app/:match*"
    }
  ]
}
